enchufar Chamuyo

el escupir_dígito
  dado n da
    escupir (
      mirar n
        si 0# da "cero"
        si 1# da "uno"
        si 2# da "dos"
        si 3# da "tres"
        si 4# da "cuatro"
        si 5# da "cinco"
        si 6# da "seis"
        si 7# da "siete"
        si 8# da "ocho"
        si 9# da "nueve"
        si no da "dígito incorrecto"
      boludo
    )

